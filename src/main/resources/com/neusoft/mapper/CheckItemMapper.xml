<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mapper.CheckItemMapper"  >
    <select id="selectAll" resultType="CheckItem">
        select * from checkitem
    </select>
    <select id="selectById" resultType="CheckItem" parameterType="int">
        select * from checkitem where id=#{cid}
    </select>
</mapper>